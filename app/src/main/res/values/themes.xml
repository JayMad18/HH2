<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.HealthyHome1" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/Pink</item>
        <item name="colorPrimaryVariant">@color/Pink</item>
        <item name="colorOnPrimary">@color/GhostWhite</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/Pink</item>
        <item name="colorSecondaryVariant">@color/GhostWhite</item>
        <item name="colorOnSecondary">@color/Pink</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorSecondary</item>
        <!-- Customize your theme here. -->
        <item name="android:windowSplashscreenContent">@drawable/ic_home_black_24dp</item>
    </style>

    <style name="Theme.HealthyHome1.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.HealthyHome1.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.HealthyHome1.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>